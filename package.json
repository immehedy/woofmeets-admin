{
  "name": "woofmeets-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@digitalroute/cz-conventional-changelog-for-jira": "^7.4.1",
    "@headlessui/react": "^1.7.3",
    "@tremor/react": "^1.1.3",
    "axios": "^0.27.2",
    "cogo-toast": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.29.3",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.1",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "react-stars": "^2.2.5",
    "react-table": "^7.8.0",
    "react-tooltip": "^5.7.2",
    "recharts": "^2.1.14",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "cm": "cz"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraOptional": true,
      "jiraPrefix": "WOOF",
      "skipScope": false,
      "jiraLocation": "post-description"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && cz --hook || true"
    }
  }
}
